   "source": [
    "import math\n",
    "\n",
    "# Nhập cạnh tam giác đều\n",
    "a = float(input(\"Nhập cạnh tam giác đều: \"))\n",
    "\n",
    "# Nửa chu vi\n",
    "p = (a + a + a) / 2\n",
    "\n",
    "# Áp dụng công thức Heron\n",
    "S = math.sqrt(p * (p - a) * (p - a) * (p - a))\n",
    "\n",
    "print(\"Diện tích tam giác đều là:\", S)\n"
   ]
